@charset "utf-8";

body { background: rgb(255, 255, 255); }

body.dark { background: rgb(25, 25, 25); }

@keyframes startup-shimmer-animation {=20
  0% { transform: translateX(-100%) translateZ(0px); }
  100% { transform: translateX(100%) translateZ(0px); }
}

@keyframes startup-shimmer-fade-in {=20
  0% { opacity: 0; }
  100% { opacity: 1; }
}

@keyframes startup-spinner-rotate {=20
  0% { transform: rotate(0deg) translateZ(0px); }
  100% { transform: rotate(360deg) translateZ(0px); }
}

#initial-loading-spinner { position: fixed; height: 100vh; width: 100vw; z-=
index: -1; display: none; align-items: center; justify-content: center; opa=
city: 0.5; }

#initial-loading-spinner svg { height: 24px; width: 24px; animation: 1s lin=
ear 0s infinite normal none running startup-spinner-rotate; transform-origi=
n: center center; pointer-events: none; }

#skeleton { background: rgb(255, 255, 255); position: fixed; height: 100vh;=
 width: 100vw; z-index: -1; display: none; overflow: hidden; }

#initial-loading-spinner.show, #skeleton.show { display: flex; }

body.dark #skeleton { background: rgb(25, 25, 25); }

.notion-front-page #skeleton, .notion-mobile #skeleton { display: none; }

#skeleton-sidebar { background-color: rgb(251, 251, 250); box-shadow: rgba(=
0, 0, 0, 0.024) -1px 0px 0px 0px inset; display: flex; width: 240px; flex-d=
irection: column; padding: 12px 14px; overflow: hidden; }

body.dark #skeleton-sidebar { background-color: rgb(32, 32, 32); box-shadow=
: rgba(255, 255, 255, 0.05) -1px 0px 0px 0px inset; }

#skeleton.isElectron #skeleton-sidebar { padding-top: 46px; }

#skeleton .row { display: flex; margin-bottom: 11px; align-items: center; }

#skeleton .row.fadein { animation: 1s ease-in 0s 1 normal both running star=
tup-shimmer-fade-in; }

#skeleton .chevron { width: 12px; height: 12px; display: block; margin-righ=
t: 4px; fill: rgba(227, 226, 224, 0.5); }

body.dark #skeleton .chevron { fill: rgb(47, 47, 47); }

.startup-shimmer { background: rgba(227, 226, 224, 0.5); overflow: hidden; =
position: relative; }

body.dark .startup-shimmer { background: rgb(47, 47, 47); }

.startup-shimmer::before { content: ""; position: absolute; height: 100%; w=
idth: 100%; z-index: 1; animation: 1s linear 0s infinite normal none runnin=
g startup-shimmer-animation; background: linear-gradient(90deg, rgba(0, 0, =
0, 0) 0px, rgba(0, 0, 0, 0.03) 50%, rgba(0, 0, 0, 0) 100%); }

body.dark .startup-shimmer::before { background: linear-gradient(90deg, tra=
nsparent 0px, rgba(86, 86, 86, 0.1) 50%, transparent 100%); }

